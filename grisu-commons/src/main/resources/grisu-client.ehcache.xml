<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="10" timeToLiveSeconds="20"
		diskPersistent="true" />

	<cache name="filesystem" maxElementsInMemory="10000" eternal="false"
		timeToLiveSeconds="10" overflowToDisk="false"
		diskSpoolBufferSizeMB="0" maxElementsOnDisk="0" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="0" memoryStoreEvictionPolicy="LRU" />


	<cache name="short" maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="180" timeToLiveSeconds="240" overflowToDisk="false"
		diskSpoolBufferSizeMB="0" maxElementsOnDisk="0" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="0" memoryStoreEvictionPolicy="LRU" />

</ehcache>
