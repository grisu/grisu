				<exact>1</exact>
			</TotalCPUCount>
		</Application>
		<Resources>
			<CandidateHosts>
			</CandidateHosts>
		</Resources>
		<DataStaging>
			<FileName />
			<FileSystemName />
			<Source>
				<URI>gsiftp://ng2.canterbury.ac.nz/home/grid-admin/C_AU_O_APACGrid_OU_VPAC_CN_Markus_Binsteiner/grix_splash_v1.1.jpg</URI>
			</Source>
